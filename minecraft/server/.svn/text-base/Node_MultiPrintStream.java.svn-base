		secondStream.print(x);
	}
	
	public void print(long x) {
		super.print(x);
		secondStream.print(x);
	}
	
	public void print(Object x) {
		super.print(x);
		secondStream.print(x);
	}
	
	public void print(String x) {
		super.print(x);
		secondStream.print(x);
	}
	
	
	public void println(boolean x) {
