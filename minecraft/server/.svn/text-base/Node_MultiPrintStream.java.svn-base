package net.minecraft.src;

import java.io.OutputStream;
import java.io.PrintStream;

public class Node_MultiPrintStream extends PrintStream {

	PrintStream secondStream;
	
	public Node_MultiPrintStream(OutputStream out,PrintStream secondStream) {
		super(out);
		this.secondStream = secondStream;
	}
	
	public void print(boolean x) {
		super.print(x);
		secondStream.print(x);
	}
	
	public void print(char x) {
		super.print(x);
		secondStream.print(x);
	}
	
	public void print(char[] x) {
		super.print(x);
		secondStream.print(x);
	}
	
